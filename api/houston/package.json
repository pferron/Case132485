{
  "name": "@scc/houston-api",
  "description": "Houston proxy API",
  "main": "server.js",
  "author": "Houston",
  "version": "1.0.0",
  "versionTest": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "dev": "nodemon --watch 'src/**' --ext 'ts js json' --exec 'ts-node' ./src/server-cluster.ts",
    "dev-node": "ts-node ./src/server-cluster.ts",
    "start": "node dist/server-cluster.js",
    "build": "rm -rf dist && tsc",
    "betterer": "betterer",
    "betterer:ci": "betterer ci",
    "resetDependencies": "rm -rf node_modules",
    "precommit": "pnpm lint && npx tsc -p ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.485.0",
    "@aws-sdk/lib-storage": "3.485.0",
    "@smithy/node-http-handler": "2.3.1",
    "axios": "1.6.7",
    "body-parser": "1.20.2",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "form-data": "4.0.0",
    "https": "1.0.0",
    "luxon": "3.4.4",
    "multer": "1.4.5-lts.1",
    "node-cache": "5.1.2",
    "node-schedule": "2.1.0",
    "os": "^0.1.2",
    "pino": "8.17.2",
    "qs": "6.11.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@scc/eslint-config": "workspace:*",
    "@scc/typescript-config": "workspace:*",
    "@aws-sdk/types": "3.502.0",
    "@types/body-parser": "1.19.5",
    "@types/express": "4.17.21",
    "@types/multer": "1.4.11",
    "@types/luxon": "3.3.0",
    "@types/node-schedule": "2.1.0",
    "@types/qs": "6.9.11",
    "@types/uuid": "9.0.1",
    "husky": "9.0.7",
    "pino-pretty": "10.3.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
